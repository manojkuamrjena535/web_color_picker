{
    "name":"web color picker",
    "description":"A extenstion for pickking color from the web pages ",
    "version":"1.0",
    "manifest_version":3,
   
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "activeTab",
        "scripting"
    ],
    "action":{
        "default_popup":"popup.html"
    },

    "icons":{
        "16":"images/color_picker.png",
        "32":"images/color_picker.png",
        "48":"images/color_picker.png"
    }
}